#!/bin/bash

params=(
-bmDuration 20000 
-numClients 200
-prefixSize 1000
-bucketDeletionThreads 100
-bucketPrefix salman.s3.micro.benchmarks 
-clientId 0 
#-deleteExistingData 
-disableConnectorSharing 
-disableS3TransferManager 
# -maxUploadThreads 10 
-diskNSFile /mnt/ssd/namespace.bin 
-numBuckets 1 
-objSize 1024 
-testPut 
#-testDelete 
-testGet 
#-testGetMetaData 
-testList 
#-testObjExists 
-tmpFolder "/mnt/ssd" 
-usePrefixes 
#-startPrompt
-workersStartBatchSize 10
-workersBatchStartDelay 1000
#-saveNSToDisk 
)

command="java -Xmx20G -jar ../target/s3-micro-benchmark-0.1-jar-with-dependencies.jar "

