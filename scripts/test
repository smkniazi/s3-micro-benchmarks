#!/bin/bash

params=(
-bmDuration 20000 
-numClients 100
-bucketDeletionThreads 200
-bucketPrefix salman.s3.micro.benchmarks 
-clientId 0 
-deleteExistingData 
-disableConnectorSharing 
-disableS3TransferManager 
#-numTransferManagerThreads 1
-diskNSFile /mnt/ssd/namespace.bin 
-numBuckets 1 
-objSize 1024 
-testPut 
-testGet 
#-testDelete 
#-testGetMetaData 
#-testList 
#-testObjExists 
-tmpFolder "/mnt/nvme_ssd0/tmp" 
-usePrefixes 
-noOfPrefixes 1
#-startPrompt
-workersStartBatchSize 10
-workersBatchStartDelay 1000
#-saveNSToDisk 
-region eu-west-1
)

command="java -Xmx20G -jar ../target/s3-micro-benchmark-0.1-jar-with-dependencies.jar "

for i in ${params[@]}; do 
   command="$command $i" 
done

echo $command
$command
